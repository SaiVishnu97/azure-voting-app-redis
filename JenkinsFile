@Library('https://github.com/devbyaccident/demo-shared-pipeline')_


pipeline {
    agent any

    stages {
        stage('Verify Build') {
            steps {
                echo "$GIT_BRANCH"
            }
        }
        stage('call hello world from a shared library') {
         steps {
           script
           {
           helloWorld()
           }
         }
      }
    }
}
